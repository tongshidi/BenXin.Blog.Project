{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Cors": {
    "PolicyName": "CorsIpAccess", //策略名称
    "EnableAllIPs": true,
    // 支持多个域名端口，注意端口号后不要带/斜杆：比如localhost:8000/，是错的
    // 注意，http://127.0.0.1:1818 和 http://localhost:1818 是不一样的
    "IPs": ""
  },
  "AllowedHosts": "*"

}
